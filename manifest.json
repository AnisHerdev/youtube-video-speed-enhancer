{
    "manifest_version": 3,
    "name": "YouTube Speed Booster",
    "version": "1.0",
    "description": "Set YouTube video playback speed beyond 2x",
    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/*"],
            "js": ["youtube_speed_change.js"]
        }
    ],
    "action": {
    "default_icon": {
        "16": "youtube_video_speed_icon.png",
        "48": "youtube_video_speed_icon.png",
        "128": "youtube_video_speed_icon.png"
    }
    },
    "permissions": []
  }
  